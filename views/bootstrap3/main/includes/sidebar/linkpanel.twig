{% if linkpanel_links %}
	<div class="panel panel-primary">
		<div class="panel-heading panel-title">{{ linkpanel_title }}</div>
		<div class="panel-body linkblock">
			{% for link in linkpanel_links %}
				<div class="icon-{{ link.game.alias }}">
					<a title="[{{ link.game.name }}] {{ link.title|raw }}" href="{{ link.url }}">{{ link.title|raw }}</a>
					{% if link.addendum %}({{ link.addendum }}){% endif %}
				</div>
			{% endfor %}
		</div>
	</div>
{% endif %}
