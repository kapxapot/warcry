{% macro tree(items) %}
    {% import _self as m %}
	{% if items %}
		<ul>
		    {% for i in items %}
		        <li>
		            <a href="{{ i.url }}">{{ i.title }}</a>
	                {{ m.tree(i.items) }}
		        </li>
		    {% endfor %}
    	</ul>
    {% endif %}
{% endmacro %}
